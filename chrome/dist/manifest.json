{
  "manifest_version": 3,
  "name": "bandog",
  "version": "1.0.1",
  "description": "Bandog Extension",
  "icons": {
    "16": "bandog-logo-white.png",
    "48": "bandog-logo-white.png",
    "128": "bandog-logo-white.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*"
      ],
      "js": [
        "bandog-extension.js"
      ]
    }
  ],
  "permissions": []
}